<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.POJO.ClazzInfo" table="clazz_info" schema="courseregistration">
        <id name="classInfoId">
            <column name="class_info_id" sql-type="int"/>
        </id>
        <property name="class_id">
            <column name="class_id" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="student_id">
            <column name="student_id" sql-type="char(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>